{
  "moduleFileExtensions": [
    "ts",
    "json",
    "js"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": [
      "@swc/jest"
    ]
  },
  "moduleNameMapper": {
    "^~/(.*)$": "<rootDir>/../$1"
  },
  "testTimeout": 25000
}
